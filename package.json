{
  "name": "@js-utilities/typecheck",
  "version": "0.1.6",
  "description": "Utility library for javascript type checking.",
  "license": "MIT",
  "author": {
    "name": "Vitalii Shapovalov",
    "email": "<shapovalov.vitali.v@gmail.com>"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vitalishapovalov/Typecheck.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "type checking",
    "type predicates",
    "isCallable",
    "isFunction",
    "isArrowFunction",
    "isArray",
    "isObject",
    "isBoolean",
    "isUndefined",
    "isNumber",
    "isString",
    "isSymbol",
    "isNull",
    "isMap",
    "isSet",
    "isWeakSet",
    "isWeakMap"
  ],
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "types": "dist/types/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "rollup -c rollup.config.ts --name typecheck",
    "test": "jest --coverage",
    "lint": "tslint -c tslint.ts 'src/**/*.ts'",
    "clean": "rm -rf dist && rm -rf .rpt2_cache && rm -rf coverage && rm -rf ./node_modules",
    "prepublishOnly": "npm run lint && npm run test && npm run clean && npm i && npm run build"
  },
  "devDependencies": {
    "@jest-decorated/core": "0.1.7",
    "@types/jest": "^27.0.2",
    "@types/node": "^18.11.5",
    "jest": "^27.3.1",
    "rollup": "^2.26.10",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.3",
    "rollup-plugin-sourcemaps": "^0.6.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "ts-jest": "^27.0.7",
    "tslint": "^5.18.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^4.0.2"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": {
          "target": "es6"
        }
      }
    },
    "transform": {
      ".ts": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "/__tests__/.*.spec.(ts|js)$",
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/",
      "/dist/"
    ]
  }
}
